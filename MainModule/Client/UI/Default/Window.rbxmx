					<ProtectedString name="Source"><![CDATA[service = nil;
client = nil;

local function ignore(child)
    if child.Parent and child.Parent.Parent then
        if child.Parent.Parent.Name == "TabFrame" then
            return true;
        end
    end

    if child.BackgroundTransparency == 1 then
        return true;
    end
end

local rotationSpeed = 0.2

local function propChange(child, target, list)
    for i in next,list do
        if target[i] ~= child[i] then
            target[i] = child[i];
        end
    end

    if child.ZIndex == 1 then
        child.ZIndex = 2;
    end

    target.ZIndex = child.ZIndex-1;
end





return function(data)
    local gui = script.Parent.Parent;--data.GUI;
	local grandient = gui.Drag.Main:FindFirstChild("UIGradient")
	
	if not grandient then
		return
	end
	
	-- There is a weird bug that causes this Modifier to be loaded for windows from other themes
	
	local c1 = client.Variables.CustomThemeColor1
	local c2 = client.Variables.CustomThemeColor2
	grandient.Color = ColorSequence.new(Color3.new(c1[1], c1[2], c1[3]),Color3.new(c2[1], c2[2], c2[3]))
    local function updateRotation()
        local rotation = grandient.Rotation + rotationSpeed
        grandient.Rotation = rotation
    end
    task.spawn(function()
        game:GetService("RunService").RenderStepped:Connect(updateRotation)
    end)
    local function apply(child)
        --pcall(function() child.BackgroundColor3 = Color3.new(0.054902, 0.509804, 1) end)
        local rounder = service.New("UICorner",{
            CornerRadius = UDim.new(0, 3);
            Parent = child;
        });

        if child.Name == "__TITLEBUTTON" then
            rounder.CornerRadius = UDim.new(0,3);
            --elseif child:IsA("ScrollingFrame") and not ignore(child) then
            --		child.BackgroundTransparency = 1;

        end
    end
    gui.DescendantAdded:Connect(function(child)
        if child:IsA("GuiObject") and not child:FindFirstChildOfClass("UICorner") and not ignore(child) then
            --task.wait(1)
            apply(child);
        end
        local object =gui.Drag.Main --path to object	
    end)

end]]></ProtectedString>
